set autolist
set color
set complete = igncase
set correct = cmd
set histfile = ~/.tcsh_history
set fignore = (.o .\~)
set nobeep
set notify
set promptchars = ($ \#)
set prompt = "%{\e[32;1m%} %n %{\e[37;1m%}(on %m) %{\e[31;1m%}@ %{\e[34;1m%}[%~] %# \n %{\e[31;1m%}>%{\e[00m%} "
set savehist = (100000 merge)
set time = (10 "Process took %E to complete.")

alias cp "cp -v"
alias mkdir "mkdir -pv"
alias mv "mv -v"
alias rm "rm -v"
alias rmdir "rmdir -v"
alias ll "ls -l"
alias la "ls -A"
alias lla "ls -Al"
